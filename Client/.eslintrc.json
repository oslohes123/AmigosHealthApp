{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "ignorePatterns": [
        "node_modules/*",
        ".next/*",
        ".out/*",
        "!.prettierrc.js",
        "!.eslintrc.js"
    ],
    "plugins": [
        "react"
    ],
    "rules": {
        "linebreak-style": "off",
        "array-callback-return": "off",
        "react/prop-types": "off",
        "react/no-array-index-key": "off",
        "no-alert": "off",
        "import/no-extraneous-dependencies": "off"
    }
}